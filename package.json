    "development": [
      "last 3 Chrome versions",
      "last 3 Firefox versions",
      "iOS >= 7",
      "Android >= 7"
    ]
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "node-sass": "^4.13.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-scripts": "3.3.0",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@testing-library/dom": "^6.11.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^8.0.3",
    "@types/classnames": "^2.2.9",
